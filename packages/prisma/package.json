    "@prisma/client": "^6.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "prisma": "^6.7.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
